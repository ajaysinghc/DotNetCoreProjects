@model IEnumerable<Gig>
@{
    ViewData["Title"] = "Home Page";
}

<h1>Upcoming Gigs</h1>
<div class="container">
    <ul class="giggs">
    @foreach(var gig in Model)
    {
      <li>
          <div class="venue">  
            @gig.Venue  
            <div class="date">
               @gig.DateTime.ToString("d MMM") 
          </div>       
          </div>
          
          <div class="detail">
              <span class="artist">@gig.Artist.UserName </span>
              <span class="genre">@gig.Genre.Name</span>
          </div>
      </li>   
    }
    </ul>

</div>